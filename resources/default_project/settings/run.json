{
    // Template for the message shown when a test passes.
    // See templates/run/success.txt.
    //
    "success_message": "templates/run/success.txt",

    // Template for the message shown when a test fails.
    // See templates/run/failure.txt.
    //
    "failure_message": "templates/run/failure.txt",

