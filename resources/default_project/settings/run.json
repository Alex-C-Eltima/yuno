    // See templates/run/failure-diff.txt
    //
    "diff_message": "templates/run/failure-diff.txt",

    // Template for the message shown when a test is skipped.
    // See templates/run/test-skipped.txt.
    //
    "skip_message": "templates/run/test-skipped.txt",

    // Template for the message shown when something unexpected happens while a
    // test is running. See templates/run/warning.txt.
    //
    "warn_message": "templates/run/warning.txt"
}
